{
	"name": "google-drive-for-vscode",
	"displayName": "Google Drive for VS Code",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./lib/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "google-drive-for-vscode.helloWorld",
				"title": "Hello World"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "google-drive-for-vscode",
					"title": "Google Drive for VS Code",
					"icon": "resources/icon.svg"
				}
			]
		},
		"views": {
			"google-drive-for-vscode": [
				{
					"id": "drive-view",
					"name": "Google Drive"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.18.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.26.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2"
	},
	"publisher": "muiz6"
}